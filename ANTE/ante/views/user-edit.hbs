<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Drive Track</title>
  <script type="text/javascript" src="/js/jquery.min.js"></script>
  <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
  <script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="/js/home.js"></script>
  <script type="text/javascript" src="/js/user-edit.js"></script>
  <link rel="stylesheet" href="stylesheets/style.css" />
</head>


<body onload="getUser()">
    <div class="navigation col-lg-1">
        <h1 class="head-title">
          <a href="http://localhost:3000/main">
            <span class="glyphicon glyphicon-home" aria-hidden="true"></span>
               Drive Track
          </a>
        </h1>
        <nav id="navigation">
          <a href="views/employee.hbs">
            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
            Repartidores
          </a>
          <a href="views/order.hbs">
            <span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
            Pedidos
          </a>
          <a href="views/tracking.hbs">
            <span class="glyphicon glyphicon-screenshot" aria-hidden="true"></span>
            Rastreo
          </a>
        </nav>
      
        <nav id="user_logged">
          <div class="user-image">
            <img id="image-logged" src="images/users/default-admin.png"/>
          </div>
          <span id="identity_name">
          </span>
      
          <a href="http://localhost:3000/user">
            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
            Mis datos
          </a>
          <a href="#" class="btn-logout">
            <span class="glyphicon glyphicon-log-out" aria-hidden="true" onclick="salir()"></span>
            Salir
          </a>
        </nav>
      </div>    
  <div class="col-lg-6 col-lg-offset-2">
    <div class="jumbotron">
      <form class="col-lg-10">
        <h2> MIS DATOS</h2>
        </br>
        <p>
          <label>Usuario:</label>
          <input type="text" id="txtUsuario" class="form-control" required />
        </p>
        <p>
          <label>Nombre:</label>
          <input type="text" id="txtNombre" class="form-control" required />
        </p>
        <p>
          <label>Paterno:</label>
          <input type="text" id="txtPaterno" class="form-control" required />
        </p>
        <p>
          <label>Materno:</label>
          <input type="text" id="txtMaterno" class="form-control" required />
        </p>
        </br>
        <input type="button" value="Aceptar" class="btn btn-primary" />
      </form>
    </div>
  </div>
</body>
</html>